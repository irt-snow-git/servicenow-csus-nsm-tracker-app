<?xml version="1.0" encoding="UTF-8"?>
<record_update table="sys_ui_page">
    <sys_ui_page action="INSERT_OR_UPDATE">
        <category>general</category>
        <client_script/>
        <description>Page for end-users to submit new issues. </description>
        <direct>false</direct>
        <endpoint>x_casus_nsm_issue_Submit Issue.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
<style>
.btn-close {
	font-size:18px;
	color: lightgray;
	font-weight: lighter;
	background: transparent;
}
	
.btn-submit {
	float:right;
	margin-right: 30px !important;
	color: #fff !important;
    background-color: #4da845 !important;
    border-color: #418c3a !important;
}
	
</style>
	
<div ng-app="myApp">
<!-- References-->
<div class="row col-md-10" id="issue-div" style="padding:50px;">
	<div>
		<div ng-controller="Issue" id="issue2-div">

			<h1>Submit an Issue</h1>
			<p>Text goes here lol</p>

			<!-- Repeats based on amount of data -->
				<div ng-repeat="i in data.issue track by $index">
					<div class="col-lg-10 col-md-10 col-sm-12">
					<div class="panel panel-default">
						<div class="panel-heading">
							<h3 class="panel-title">New Issue</h3>
						</div>
						<div class="panel-body">
							<div class="row col-lg-6">
								<div class="form-group col-lg-9 col-lg-offset-1 col-md-4 col-sm-4 col-xs-12">
									<label for="supQuantity">Submitter</label>
									<input type="number" ng-model="i.submitter" class="form-control" id="supQuantity"/>
								</div>
								<div class="form-group col-lg-9 col-lg-offset-1 col-md-4 col-sm-4 col-xs-12">
									<label for="supType">Instrument</label>
									<input class="form-control" list="instrumentList" id="supType" ng-if="async == 1" placeholder="Type name or model number" ng-model="i.instrument_id"/>
									<div class="customDropdown"></div>
								</div>
								<div class="form-group col-lg-9 col-lg-offset-1 col-md-4 col-sm-4 col-xs-12">
									<label for="supQuantity">Issue Type</label>
									<input type="number" ng-model="i.issue_type" class="form-control" id="supQuantity"/>
								</div>
							</div>
							
							<div class="row col-lg-6">
								<div class="form-group col-lg-9 col-lg-offset-3 col-md-4 col-sm-4 col-xs-12">
									<label for="supDate">Date</label>
									<input type="date" ng-model="i.date" class="form-control" id="supDate"/>
								</div>
								<div class="form-group col-lg-9 col-lg-offset-3 col-md-4 col-sm-4 col-xs-12">
									<label for="priority">Priority</label>
									<select class="form-control" id="priority_select" ng-model="i.priority">
										<option value="3">3 - Routine (At first opportunity)</option>
										<option value="2">2 - Urgent (needs to be addressed within 24 hours)</option>
										<option value="1">1 - Critical (Classes impacted)</option>
									</select>
								</div>
								
							</div>
							<div class="row col-lg-12">
								<hr></hr>
							</div>
							
							<div class="row col-lg-12">
								<div class="form-group col-lg-10 col-lg-offset-1">
									<label for="short_description">Subject</label>
									<input type="text" class="form-control" ng-model="i.subject" id="short_description"/>
								</div>
							</div>
							<div class="row col-lg-12">
								<div class="form-group col-lg-10 col-lg-offset-1">
									<label for="notes">Notes</label>
									<textarea class="form-control" ng-model="i.notes" id="notes"></textarea>
								</div>
							</div>
						</div>
					</div>
				</div>
				</div>
			<!-- End Repeat -->
				<div class="row col-lg-10" style="padding-left:30px;">
					<button class="btn-submit" ng-click="submitAll()"> Submit </button>
				</div>
		</div> 
		<!-- END Supplies Received Controller-->
				
	</div>
	</div>
</div>

</j:jelly>]]></html>
        <name>Submit Issue</name>
